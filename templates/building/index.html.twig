{% extends "./base2.html.twig" %}

        {% block body %}


<main class="flex-1 flex bg-gray-50">
  <section class="bg-gray-50 dark:bg-gray-50  w-full">
          <div class=" sm:ml-20">
            <div class=" border-2 border-gray-200 border-dashed rounded-lg dark:border-gray-700 mt-0">

            <div class=" border-b border-gray-200">
              {#            <h1 class="text-6xl pt-12 pb-8">#}
              {#              Buiding Records ({{ building|length }})#}
              {#            </h1>#}

              {#            <span class="font-light text-gray-500 italic">#}
              {#                Created by Code With me#}
              {#            </span>#}
            </div>
          </div>
          {#        {% if app.user %}#}
          <div class="mx-auto w-4/5 my-8">
            <a
                    href="/CreateBuilding"
                    class="uppercase border border-gray-500 text-lg py-4 px-6 rounded transition transition-all bg-gray-800 text-white hover:bg-white hover:text-gray-800">
              Create New Post
            </a>
          </div>
          {#        {% endif %}#}

          <div class="md:grid lg:grid-cols-3 gap-20 w-4/5 mx-auto py-15 ">
            {% for building in buildings %}

              <div class="text-center pt-8 pb-4">
                <img
                        src="{{ building.ImagePath }}"
                        alt=""
                        class="shadow-xl rounded-md"
                />
                <h2 class="text-gray-700 font-bold text-3xl py-2">
                  {{ building.name }}
                </h2>

                <span class="text-gray-500">
              By <span class="italic text-sm text-gray-800">Building No | {{ building.code }}</span>
                </span>

                <p class="text-base text-gray-700 pt-4 pb-10 leading-8 font-light">
                  {{ building.description }}
                </p>
                <a href="/building/{{ building.id }}" class="uppercase border border-gray-500 text-gray-600 text-lg py-4 px-12 rounded transition transition-all hover:bg-gray-800 hover:text-white">
                  Keep Reading
                </a>
              </div>
            {% endfor %}
          </div>
          </div>


</section>
</main>
        {% endblock %}



















































